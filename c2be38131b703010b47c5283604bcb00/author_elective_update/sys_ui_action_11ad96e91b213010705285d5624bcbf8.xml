        <name>Assign to me</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[current.assigned_to = gs.getUserID();
if(current.state == 1)
	current.state = 10;
var gr = new GlideRecord("sys_user_grmember");
gr.addQuery("user",gs.getUserID());
gr.query();
if(gr.getRowCount() == 1){
	while(gr.next()){
		current.assignment_group = gr.getValue("group");
		current.update();
	}
}
current.update();
var url = gs.getSession().getUrlOnStack();
if(url!=null){
	var link = '<a href="sn_customerservice_case.do?sys_id=' + current.sys_id + '">' + current.number + '</a>';
	gs.addInfoMessage(gs.getMessage("Case {0} has been assigned to you.", link));
}	
else
	gs.addInfoMessage(gs.getMessage("Case {0} has been assigned to you.", current.number));]]></script>
